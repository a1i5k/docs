Option Explicit

Sub Main
  Dim TS     As Spreadsheet
  Dim n      As Integer
  Dim B1,B12 As Double
  Dim DT,MA  As Double
  Dim Sig    As Double

  Set TS = ActiveSpreadsheet

  Sig = 0.04
  B1  = 0.3776
  B12 = 0.5728

  For n = 1 To TS.NumberOfCases
     TS.Cells(n,2) = RndNormal(Sig)
  Next n

' ARIMA(0.1,1)(0,1,1)_12
' (1-z**12)(1-z){Ln(TS[n])}=(1-B12*z**12)(1-B1*z){U[n]}
  For n = 1 To 13
     DT            = TS.Cells(n,1)
     TS.Cells(n,3) = Log(DT)
     TS.Cells(n,4) = DT
  Next n

  For n = 14 To TS.NumberOfCases
     DT = TS.Cells(n-1,3) + TS.Cells(n-12,3) - TS.Cells(n-13,3)
     MA = TS.Cells(n,2)- B1*TS.Cells(n-1,2) - B12*TS.Cells(n-12,2) + B1*B12*TS.Cells(n-13,2)

     TS.Cells(n,3) = DT + MA
     TS.Cells(n,4) = Exp(TS.Cells(n,3))
  Next n
End Sub

