Option Explicit
Sub Main
  Dim TS As Spreadsheet
  Dim n  As Integer

  Set TS = ActiveSpreadsheet

  TS.Cells(1,2) = 0

  TS.Cells(1,3) = 0

  TS.Cells(1,4) = 0
  TS.Cells(2,4) = 0

  TS.Cells(1,5) = 0
  TS.Cells(2,5) = 0

  TS.Cells(1,6) = 0
  TS.Cells(2,6) = 0

  TS.Cells(1,7) = 0
  TS.Cells(2,7) = 0

  For n = 1 To TS.NumberOfCases
     TS.Cells(n,1) = RndNormal(1)
  Next n

  For n = 2 To TS.NumberOfCases
     TS.Cells(n,2) = TS.Cells(n,1) - 0.75 * TS.Cells(n-1,2)
     TS.Cells(n,3) = TS.Cells(n,1) + 0.75 * TS.Cells(n-1,3)
  Next n

  For n = 3 To TS.NumberOfCases
     TS.Cells(n,4) = TS.Cells(n,1) + 0.5 * TS.Cells(n-1,4) + 0.3 * TS.Cells(n-2,4)
     TS.Cells(n,5) = TS.Cells(n,1) - 0.5 * TS.Cells(n-1,5) + 0.3 * TS.Cells(n-2,5)
     TS.Cells(n,6) = TS.Cells(n,1) + 1.3 * TS.Cells(n-1,6) - 0.4 * TS.Cells(n-2,6)
     TS.Cells(n,7) = TS.Cells(n,1) - 1.3 * TS.Cells(n-1,7) - 0.4 * TS.Cells(n-2,7)
  Next n
End Sub

